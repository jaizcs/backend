{
	"name": "@jaizcs/backend",
	"version": "0.0.0",
	"private": true,
	"engines": {
		"node": ">=18"
	},
	"type": "module",
	"scripts": {
		"dev": "nodemon bin/server.js",
		"test": "vitest run",
		"test:watch": "vites run --watch",
		"db:start": "supabase start",
		"db:stop": "supabase stop",
		"db:diff": "supabase db diff -f",
		"db:reset": "supabase db reset",
		"db:seed": "node scripts/database.js seed",
		"db:seed:all": "node scripts/database.js seed:all",
		"db:seed:undo": "node scripts/database.js seed:undo",
		"db:seed:undo:all": "node scripts/database.js seed:undo:all",
		"db:seed:create": "node scripts/database.js seed:create",
		"db:types": "supabase gen types typescript --local > supabase/types.ts",
		"format": "prettier -uw .",
		"prepare": "husky install"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.33.1",
		"bcrypt": "^5.1.1",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.1",
		"openai": "^4.2.0",
		"tiny-invariant": "^1.3.1",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/node": "^18.17.8",
		"@vitest/coverage-v8": "^0.34.2",
		"husky": "^8.0.0",
		"lint-staged": "^14.0.1",
		"nodemon": "^3.0.1",
		"prettier": "^3.0.2",
		"supabase": "^1.88.0",
		"supertest": "^6.3.3",
		"vitest": "^0.34.2"
	},
	"lint-staged": {
		"*": "npm run format"
	}
}
